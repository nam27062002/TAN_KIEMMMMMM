    hasSpawnCharacter: 0
    spawnCharacters: []
    shake: 0
  - useAvt: 1
    avatar: {fileID: 21300000, guid: b9f0356a1487a054aa17f9403c763869, type: 3}
    text: "Kh\xF4ng. Kh\xF4ng c\xF3 g\xEC. Th\xF4i... ch\xFAng ta v\u1EC1 th\xF4i.
      R\u1ED1i Ng\u01B0\u1EDDi ch\u1EA1y th\xEC c\u0169ng ch\u1EA1y r\u1ED3i, c\xF2n
      \u0111\u1EE9ng ng\xE2y ra \u1EDF \u0111\xE2y l\xE0m g\xEC?"
    hasSpawnCharacter: 0
    spawnCharacters: []
    shake: 0
  - useAvt: 1
