    hasSpawnCharacter: 0
    spawnCharacters: []
    shake: 0
  - useAvt: 1
    avatar: {fileID: 21300000, guid: 423a9da2fb801e44eb5742257ff9abbe, type: 3}
    text: "I don\u2019t accept that, so I\u2019d like to capture you back for interrogation.
