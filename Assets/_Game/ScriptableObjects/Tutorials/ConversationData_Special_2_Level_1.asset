    hasSpawnCharacter: 0
    spawnCharacters: []
    shake: 0
  - useAvt: 1
    avatar: {fileID: 21300000, guid: 423a9da2fb801e44eb5742257ff9abbe, type: 3}
    text: "Guess we\u2019re really are saving the politics talk for later. Everyone,
