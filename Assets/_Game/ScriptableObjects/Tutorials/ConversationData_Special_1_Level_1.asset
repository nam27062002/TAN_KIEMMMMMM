    hasSpawnCharacter: 0
    spawnCharacters: []
    shake: 0
  - useAvt: 1
    avatar: {fileID: 21300000, guid: b9f0356a1487a054aa17f9403c763869, type: 3}
    text: "Unless there\u2019s something so fearsome nearby and coming our way that
